{
  "name": "ts-adt",
  "version": "2.1.2",
  "author": "Paul Gray <pfbgray@gmail.com>",
  "description": "An Algebraic Data Type generator for Typescript",
  "main": "ADT.js",
  "types": "ADT.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc && ts-node ./scripts/build",
    "prepublish": "tsc",
    "test": "ava",
    "prerelease": "yarn build",
    "release": "ts-node scripts/release"
  },
  "devDependencies": {
    "typescript": "4.9"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.15",
    "express": "^4.18.2",
    "fp-ts": "^2.9.0"
  }
}
